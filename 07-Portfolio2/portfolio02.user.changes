!PortfolioTreeReportPrinter methodsFor: 'as yet unclassified' stamp: 'FP 6/1/2023 12:08:40' prior: 50640741!
initializeWith: aPortfolio

	portfolio := aPortfolio.
	report := OrderedCollection new.
	currentLevel := 0.! !
!PortfolioTreeReportPrinter methodsFor: 'as yet unclassified' stamp: 'FP 6/1/2023 12:09:07' prior: 50641526!
treeReport

	report add: portfolio name.
	currentLevel := currentLevel +1.
	portfolio createTreeReportOn: self.
	^report! !
!PortfolioTreeReportPrinter methodsFor: 'as yet unclassified' stamp: 'FP 6/1/2023 12:09:17'!
increaseLevel

	^ currentLevel := currentLevel +1! !
!PortfolioTreeReportPrinter methodsFor: 'as yet unclassified' stamp: 'FP 6/1/2023 12:09:17' prior: 50642449!
treeReport

	report add: portfolio name.
	self increaseLevel.
	portfolio createTreeReportOn: self.
	^report! !
!PortfolioTreeReportPrinter methodsFor: 'as yet unclassified' stamp: 'FP 6/1/2023 12:10:34' prior: 50642246!
affectTreeReportWithPortfolio: aPortfolio

	|indentation|
	
	indentation := ''.
	currentLevel * 4 timesRepeat: [indentation := indentation , ' '].
	
	report add: (indentation , aPortfolio name).
	self increaseLevel.
	aPortfolio createTreeReportOn: self.! !
!PortfolioTreeReportPrinter methodsFor: 'as yet unclassified' stamp: 'FP 6/1/2023 12:10:55' prior: 50640754!
affectTreeReportWithReceptiveAccount: aReceptiveAccount

	|indentation|
	
